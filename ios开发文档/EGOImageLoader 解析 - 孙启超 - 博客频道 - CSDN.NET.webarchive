    eval(func);
    function loadad(id) {
        var url = encodeURIComponent(top.location.href);
        var cbfun = "showad" + id;
        var src = 'http://a.pongo.cn/Job/GetAdForCallBack?id=' + id + '&UrlAdParam=' + encodeURIComponent(url) + '&CallBack=' + cbfun;
